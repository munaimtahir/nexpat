components:
  schemas:
    Visit:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        token_number:
          type: integer
          readOnly: true
        visit_date:
          type: string
          format: date
          readOnly: true
        status:
          enum:
          - WAITING
          - START
          - IN_ROOM
          - DONE
          type: string
          readOnly: true
        created_at:
          type: string
          format: date-time
          readOnly: true
        updated_at:
          type: string
          format: date-time
          readOnly: true
        patient:
          type: string
        queue:
          type: integer
        patient_registration_number:
          type: string
          readOnly: true
        patient_full_name:
          type: string
          readOnly: true
        queue_name:
          type: string
          readOnly: true
      required:
      - patient
      - queue
    Patient:
      type: object
      properties:
        registration_number:
          type: string
          readOnly: true
        name:
          type: string
          maxLength: 255
        phone:
          type: string
          nullable: true
          maxLength: 20
        gender:
          enum:
          - MALE
          - FEMALE
          - OTHER
          type: string
        created_at:
          type: string
          format: date-time
          readOnly: true
        updated_at:
          type: string
          format: date-time
          readOnly: true
        last_5_visit_dates:
          type: string
          readOnly: true
      required:
      - name
    Queue:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        name:
          type: string
          maxLength: 100
        created_at:
          type: string
          format: date-time
          readOnly: true
        updated_at:
          type: string
          format: date-time
          readOnly: true
      required:
      - name
    PrescriptionImage:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        visit:
          type: integer
        drive_file_id:
          type: string
          readOnly: true
        image_url:
          type: string
          format: uri
          readOnly: true
          pattern: "^(?:[a-z0-9.+-]*)://(?:[^\\s:@/]+(?::[^\\s:@/]*)?@)?(?:(?:0|25[0-5]|2[0-4][0-9]|1[0-9]?[0-9]?|[1-9][0-9]?)(?:\\\
            .(?:0|25[0-5]|2[0-4][0-9]|1[0-9]?[0-9]?|[1-9][0-9]?)){3}|\\[[0-9a-f:.]+\\\
            ]|([a-z\xA1-\uFFFF0-9](?:[a-z\xA1-\uFFFF0-9-]{0,61}[a-z\xA1-\uFFFF0-9])?(?:\\\
            .(?!-)[a-z\xA1-\uFFFF0-9-]{1,63}(?<!-))*\\.(?!-)(?:[a-z\xA1-\uFFFF-]{2,63}|xn--[a-z0-9]{1,59})(?<!-)\\\
            .?|localhost))(?::[0-9]{1,5})?(?:[/?#][^\\s]*)?\\z"
        created_at:
          type: string
          format: date-time
          readOnly: true
      required:
      - visit
    AuthToken:
      type: object
      properties:
        username:
          type: string
          writeOnly: true
        password:
          type: string
          writeOnly: true
        token:
          type: string
          readOnly: true
      required:
      - username
      - password
